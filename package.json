{
  "name": "webhook-test-server",
  "version": "1.0.0",
  "description": "Servidor de teste para webhooks",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build:front": "tsc --project tsconfig.app.json",
    "build:back": "tsc --project tsconfig.node.json",
    "build": "npm run build:front && npm run build:back && vite build",
    "preview": "vite preview",
    "dev:all": "concurrently \"node server.js\" \"vite\"",
    "job": "node processScheduledMessages.js",
    "start:job": "node processScheduledMessages.js",
    "deploy:build": "npm run build && npm start"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hapi/boom": "^10.0.1",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/react": "^19.1.3",
    "@types/react-router-dom": "^5.3.3",
    "@whiskeysockets/baileys": "^6.17.16",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "lucide-react": "^0.510.0",
    "next": "14.1.0",
    "node-fetch": "^2.6.7",
    "openai": "^4.98.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.0",
    "react-toastify": "^11.0.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}